{
  "presets": [
    ["@babel/preset-env", {"modules": false}] // 开始摇树功能
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    ["@babel/plugin-proposal-class-properties", {"loose": true}], // 松散模式 就是不严格的模式
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,  // 启动polyfill
        "helpers": true,
        "regenerator": true,
        "useESModules": true
      }
    ]
  ] 
}