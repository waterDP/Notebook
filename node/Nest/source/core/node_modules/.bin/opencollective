#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_@nuxtjs_opencollective@0.2.2@@nuxtjs/opencollective/bin/opencollective.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_@nuxtjs_opencollective@0.2.2@@nuxtjs/opencollective/bin/opencollective.js" "$@"
  ret=$?
fi
exit $ret
